/* ==UserStyle==
@name           AutomationAnywhere - UI adjustments
@namespace      Jamir-boop
@version        0.1.18
@description    Font size customization for automationanywhere CR 36.0.0
@author         jamir-boop
@preprocessor   stylus

@var checkbox customPaletteButtons "Activate this if using userJS ‚öô" 0
@var checkbox runButton "RUN button style üî•" 0
@var checkbox pathFinder "Hide sidebar" 0
@var checkbox bgStyle "Background" 1
@var checkbox fontStyle "Bigger font" 0
@var checkbox loadingCat "Loading cat üêà" 0
@var checkbox foldersStyle "Folders üìà" 0
@var checkbox editorTabsButtons "Hide editor tabs button (Flow & List & Dual)" 1
@var checkbox dialogStyle "Bot running dialog to bottom left" 0
@var text     userBg  'Loading gif base 64' url("data:image/gif;base64,") no-repeat
@var select   userBgSize 'Loading gif size' {
    'contain': 'contain',
    'cover': 'cover',
    'auto': 'auto'
}
@var color    backgroundColor3  "Gradient 1"   rgba(255, 223, 85, .2)
@var color    backgroundColor2  "Gradient 2"   rgba(255, 179, 71, .2)
@var color    backgroundColor1  "Gradient 3"   rgba(255, 105, 97, .2)

==/UserStyle== */

// Mixin for the folders containing task bots
adjustDatatableColumns()
	.datatable-header-container:not(.modal-form-content .datatable-header-container),
	.datatable-row:not(.modal-form-content .datatable-row)
		.datatable-column:nth-child(4):not(.modal-form-content .datatable-column),
		.datatable-column:nth-child(6):not(.modal-form-content .datatable-column),
		.datatable-column:nth-child(7):not(.modal-form-content .datatable-column)
			display: none !important
			background: red

// Common adjustments for datatable columns, varying by context
datatableCommonAdjustments(widthValue)
	.datatable-header-container:not(.modal-form-content .datatable-header-container),
	.datatable-row:not(.modal-form-content .datatable-row)
		.datatable-column:nth-child(3):not(.modal-form-content .datatable-column)
			width: widthValue !important
		.datatable-column:nth-child(2):not(.modal-form-content .datatable-column)
			width: 40px !important

customLoadingGifBase64()
	background: userBg
	background-size: userBgSize
	background-repeat: no-repeat

darkGradientBackground()
	background: radial-gradient(circle, #ff8c002b 20%, transparent 20%), radial-gradient(circle, #8000801c 20%, transparent 20%), radial-gradient(circle, #ff450014 20%, transparent 20%), linear-gradient(45deg, #ff8c0024 0%, #80008038 50%, #ff450003 100%)
	background-size: 10px 10px, 10px 10px, 10px 10px, auto
	background-position: 0 0, 5px 5px, 2.5px 2.5px, 0 0

customGradientBackground()
	background: radial-gradient(circle, backgroundColor1 20%, transparent 20%), radial-gradient(circle, backgroundColor2 20%, transparent 20%), radial-gradient(circle, backgroundColor3 20%, transparent 20%), linear-gradient(45deg, backgroundColor1 0%, backgroundColor2 50%, backgroundColor3 100%)
	background-size: 10px 10px, 10px 10px, 10px 10px, auto
	background-position: 0 0, 5px 5px, 2.5px 2.5px, 0 0

cursiveCascadiaFont()
	font-family: Cascadia Code monospace !important
	font-feature-settings: 'ss01' !important
	font-style: italic

// Run button animations
// Define shaking animation without scaling
@keyframes shakeSideToSide
		shakeSize = 2px
		10%,
		30%,
		50%,
		70%,
		90%
				transform: translateX(- shakeSize)

		20%,
		40%,
		60%,
		80%
				transform: translateX(shakeSize)

@keyframes pulse
		0%
				transform: scale(1)

		50%
				transform: scale(1.15)

		100%
				transform: scale(1)

// Global variables
generalFontSize = 1rem
normalFontSize = .75rem

@-moz-document regexp(".*automationanywhere\\.digital.*")
		// Grab cursor style
	.editor-palette-item__child--is_draggable
		cursor: pointer !important
	.editor-palette-item__child--is_draggable:active
		cursor: url('data:image/png;base64,') 10 10, grab !important

	// Close button change the white bg to the same of run button
	button[name="close"]
	.command-button
			background: transparent !important
			&:hover
					background: var(--color_background_interactive_whisper_hover) !important

	.loadable__overlay
		customGradientBackground()


	if fontStyle
				.editor-page__body *:not(span):not(button):not(.field-label__help):not(.help-content .taskbot-node-details__description):not(.taskbot-canvas-list-node__number)
						font-size: inputFontSize px !important
						font-size: .94rem !important

				.folder-list__content .rio-link__label,
		.folder-list__content .clipped-text__string--for_presentation,
				.message__main *:not(span):not(button):not(.field-label__help):not(.checkbox-input__content):not(.message__content)
						font-size: 1rem !important

				.code-input *
						font-size: 1.10rem !important
						line-height: normal !important
		.code-input
			all: unset !important
			font-weight: 500 !important

		code,
		.taskbot-canvas-list-node__title--disabled .taskbotnodelabel,
		.code-input,
		.editor-page-header__title-text
			cursiveCascadiaFont()

	if bgStyle
				// Editor background
				.tabs-labels,
				.editor-layout__toolbar,
				.editor-details,
				.taskbot-canvas-list,
				.editor-layout__right,
				.editor-layout__details,
				.editor-details__header::after,
				.page
						background: transparent !important

				.editor-page,
				.main-layout__content // folder background
						customGradientBackground()

				.editor-details__header
					background: rgb(255,255,255);
					background: linear-gradient(90deg, rgba(255,255,255,1) 1%, rgba(255,255,255,0) 100%);

	if editorTabsButtons
			div.editortabs
				display: none

	if runButton
			button[aria-label="Run"][name="run"]
					transition: transform 0.3s ease !important
					top: -1px // to compensate the folder below
					border: var(--color_background_interactive) 1px solid !important
					cursor: pointer !important
					animation: pulse 3s ease-in-out infinite !important
					cursiveCascadiaFont()
					&:hover
							animation: shakeSideToSide 1s ease-in-out infinite !important
							transform: scale(1.15)
					.clipped-text
						font-weight: bold

	if dialogStyle
			// Style of dialog boxes
			.modal__content:not(.finder-prompt)
					position: absolute
					//top: 60vh
					left: 0
					bottom: 0

			.message__content p
					font-size: normalFontSize !important
					line-height: normalFontSize

		.modal__backdrop, .modal-form
				darkGradientBackground()
				opacity: 1
				// filter: brightness(0.0)

		// Text inputs to text area
		.textinput-cell-input-content
						width: 100% !important
						max-height: 400px !important
						overflow: auto !important
						background-color: white !important
						box-sizing: border-box !important
						white-space: pre-wrap !important /* Preserves whitespace and line breaks */
						word-wrap: break-word !important /* Ensures long words do not overflow */

	if loadingCat
		// bigger loading image space
		.devicechannelmodal-icon.g-override
			width: 120px !important
			height: 120px !important

		// Loading animation replace
		.devicechannelmodal .icon-image-container,
		.rio-spinner--variant_LOADING,
		.rio-spinner--variant_WORKING
			*:not(img)
				display: none !important

			customLoadingGifBase64()


	// Actions and variables sidebar

	// hide the contents of the non active toolbars
	.editor-palette__accordion:not(.editor-palette__accordion--is_active)
		.editor-palette-section__scroller
				display: none
		// make the header text smaller
		span.editor-palette-section__header-title
			font-size: 10px
			font-weight: normal
			:hover
				font-weight: bold

		.editor-palette-section__action-search,
		.editor-palette-section__header-button
			padding-inline: 0px

	.editor-palette
		span.editor-palette-section__header-title
			font-size: 1rem

		flex-direction: row
		&-section__scroller
			display: block
		&__accordion
			//all: unset
			//flex: 0 0 3px important

			&--is_active
				background: red !important
				display: block

		&-section__header-show
			display: none

		//hide the scrollbars
		&-section__scroller
				overflow-y: auto
				scrollbar-width: none // For Firefox
			&::-webkit-scrollbar
				display: none // For Chrome, Safari, and Opera
		&-section__header-actions
			z-index: 999
			margin-top: 45px

		// mueve boton de limpiar busqueda a la derecha
		&-search__cancel
			right: 100%

	if customPaletteButtons
		// Hides the non active palettes
		.editor-palette__accordion--is_active
			flex: 1 0 1px !important

		.editor-palette__accordion
			flex-basis: 0px !important
			transition none
		.editor-palette span.editor-palette-section__header-title
			color white

		.editor-palette-section__header
			height 0px !important
		.editor-palette-section__header-inner
			height 12px !important

		.editor-palette-section__scroller--variant_search
			margin-top -30px !important
		.editor-palette-section__scroller-top
			background transparent !important

	if pathFinder
		.pathfinder, .main-layout__navigation
			display:none
		.main-layout__content
			padding-left: 0 !important

	// login background
	.main-layout__authentication__wrapper
		customGradientBackground()

@-moz-document regexp(".*automationanywhere\\.digital.*?folders.*$")

	if foldersStyle
			adjustDatatableColumns()
			datatableCommonAdjustments(600px)

			// Folders menu
			// Adds scroll and stick top
			.tabs
				overflow: unset

			.folder-list__items
					max-height: 100vh
					overflow-y: auto
					position: sticky
					top: 10px
					z-index: 99999

			.datatableheader-label
				flex-direction: row-reverse
			.pagetitle-label
				cursiveCascadiaFont()

@-moz-document regexp(".*automationanywhere\\.digital.*process.*$")
	// fixed 'browse' button overlaping some stuff in the process editor
	.editor-init-form-property .editor-init-form-property__path-field .command-button
		position: relative